{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "VITE_API_BASE_URL": "@vite_api_base_url",
    "VITE_CONTACT_EMAIL": "@vite_contact_email",
    "VITE_PHONE_NUMBER": "@vite_phone_number",
    "VITE_FACEBOOK_URL": "@vite_facebook_url",
    "VITE_INSTAGRAM_URL": "@vite_instagram_url",
    "VITE_YOUTUBE_URL": "@vite_youtube_url",
    "VITE_GA_TRACKING_ID": "@vite_ga_tracking_id"
  }
}